@*#fd7e14*@
<!doctype html>
<html lang="en">
<head>
    <title>@ViewData["Title"] | Online Banking System</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet">
    <link rel="stylesheet" href="~/Web/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/Web/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Web/css/jquery-ui.css">
    <link rel="stylesheet" href="~/Web/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Web/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Web/css/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/Web/css/jquery.fancybox.min.css">

    <link rel="stylesheet" href="~/Web/css/bootstrap-datepicker.css">

    <link rel="stylesheet" href="~/Web/fonts/flaticon/font/flaticon.css">

    <link rel="stylesheet" href="~/Web/css/aos.css">

    <link rel="stylesheet" href="~/Web/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script>

</head>
<style>
    .navbar{;
        background-color:white;
    }
    .navbar .navbar-nav .nav-item .nav-link{
        color: #fd7e14 !important
    }
    
        
 </style>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">


    <div id="overlayer"></div>
    <div class="loader">
        <div class="spinner-border text-primary" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>


    <div class="site-wrap">

        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>


        <nav class="navbar navbar-expand-lg navbar-light fixed-top w-100 shadow">
            <div class="container">
                <div class="col-6 col-xl-2">
                    <h1 class="mb-0 site-logo"><a href="@Url.Action("Index", "Banking")" class="h2 mb-0"><span class="text-primary">i</span>Banker </a></h1>
                </div>
                <button class="navbar-toggler"
                        type="button"
                        data-mdb-toggle="collapse"
                        data-mdb-target="#navbarRightAlignExample"
                        aria-controls="navbarRightAlignExample"
                        aria-expanded="false"
                        aria-label="Toggle navigation">
                    <i class="fas fa-bars"></i>
                </button>

                <div class="collapse navbar-collapse" id="navbarRightAlignExample">
                    <ul class="navbar-nav ml-auto mb-2 mb-lg-0 ">
                        <li class="nav-item">
                            <a class="nav-link active link" aria-current="page" href="@Url.Action("Index", "Banking")">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active link" aria-current="page" href="@Url.Action("AboutUs", "Banking")">AboutUs</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active link" aria-current="page" href="@Url.Action("OurServices", "Banking")">OurServices</a>
                        </li>
                        <li class="nav-item" style="margin:auto; margin-left:10px;">
                            <a class="nav-link text-dark" style="padding:0px;" id="register" asp-area="Identity" asp-page="/Account/Register">Register</a>
                        </li>
                        <li class="nav-item" style="margin:auto; margin-left:15px;">
                            <a class="nav-link text-dark" style="padding:0px;" id="login" asp-area="Identity" asp-page="/Account/Login">Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


        @RenderBody()

        <footer class="site-footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-9">
                        <div class="row">
                            <div class="col-md-5">
                                <h2 class="footer-heading mb-4">About Us</h2>
                                <p>
                                    iBanker is a digital banking system used worldwide in commerce, finance, and banking sectors. It provides robust and swift financial services, encompassing online transactions, fund transfers, and account management.
                                </p>
                            </div>
                            <div class="col-md-3 ml-auto">
                                <h2 class="footer-heading mb-4">Quick Links</h2>
                                <ul class="list-unstyled">
                                    <li><a href="#about-section" class="smoothscroll">Home</a></li>
                                    <li><a href="#about-section" class="smoothscroll">About Us</a></li>
                                    <li><a href="#services-section" class="smoothscroll">OurServices</a></li>
                                    <li><a href="#testimonials-section" class="smoothscroll">Register</a></li>
                                    <li><a href="#contact-section" class="smoothscroll">Login</a></li>
                                </ul>
                            </div>
                            <div class="col-md-3 footer-social">
                                <h2 class="footer-heading mb-4">Follow Us</h2>
                                <a href="#" class="pl-0 pr-3"><span class="icon-facebook"></span></a>
                                <a href="#" class="pl-3 pr-3"><span class="icon-twitter"></span></a>
                                <a href="#" class="pl-3 pr-3"><span class="icon-instagram"></span></a>
                                <a href="#" class="pl-3 pr-3"><span class="icon-linkedin"></span></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <h2 class="footer-heading mb-4">Subscribe Newsletter</h2>
                        <form action="#" method="post" class="footer-subscribe">
                            <div class="input-group mb-3">
                                <input type="text" class="form-control border-secondary text-white bg-transparent" placeholder="Enter Email" aria-label="Enter Email" aria-describedby="button-addon2">
                                <div class="input-group-append">
                                    <button class="btn btn-primary text-black" type="button" id="button-addon2">Send</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="row pt-5 mt-5 text-center">
                    <div class="col-md-12">
                        <div class="border-top pt-5">
                            <p class="copyright">
                                <small>
                                    Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | <span class="text-primary">iBanker</span>
                                </small>
                            </p>

                        </div>
                    </div>

                </div>
            </div>
        </footer>

    </div>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    @*<script src="~/Web/js/jquery-3.3.1.min.js"></script>*@
    <script src="~/Web/js/jquery-ui.js"></script>
    <script src="~/Web/js/popper.min.js"></script>
    <script src="~/Web/js/bootstrap.min.js"></script>
    <script src="~/Web/js/owl.carousel.min.js"></script>
    <script src="~/Web/js/jquery.countdown.min.js"></script>
    <script src="~/Web/js/jquery.easing.1.3.js"></script>
    <script src="~/Web/js/aos.js"></script>
    <script src="~/Web/js/jquery.fancybox.min.js"></script>
    <script src="~/Web/js/jquery.sticky.js"></script>
    <script src="~/Web/js/isotope.pkgd.min.js"></script>
    <script src="~/Web/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>






